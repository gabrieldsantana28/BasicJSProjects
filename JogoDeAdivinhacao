var numSecreto = parseInt(Math.random() * 101);
var qntTentativas = 0;

while (chute != 48 && qntTentativas != 20) {
  qntTentativas++;
  alert(qntTentativas + "a tentativa")
  var chute = prompt("Insira um nÃºmero de 0 a 100:")

  if (chute > numSecreto) {
    alert("Chute mais baixo!")
  } else if (chute < numSecreto) {
    alert("Chute mais alto!")
  } else {
    alert("Acertou!")
    break;
  }
}
